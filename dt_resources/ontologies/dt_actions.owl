<?xml version="1.0"?> 
<rdf:RDF xmlns="urn:absolute:ontologenius#" 
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:onto="ontologenius#">
    <owl:Ontology rdf:about="urn:absolute:ontologenius"/>



    <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Action Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->




    <!-- urn:absolute:ontologenius#HtnAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#HtnAction">
    </owl:Class>



    <!-- urn:absolute:ontologenius#IDLE -->

    <owl:Class rdf:about="urn:absolute:ontologenius#IDLE">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#HtnAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#PhysicalAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#PhysicalAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#HtnAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#CommunicateAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#CommunicateAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#HtnAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#PhysicalCommunication -->

    <owl:Class rdf:about="urn:absolute:ontologenius#PhysicalCommunication">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#CommunicateAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#VerbalCommunication -->

    <owl:Class rdf:about="urn:absolute:ontologenius#VerbalCommunication">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#CommunicateAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#Wait -->

    <owl:Class rdf:about="urn:absolute:ontologenius#Wait">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#HtnAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#PickAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#PickAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PhysicalAction"/>
        <rdfs:hasEffect rdf:resource="urn:absolute:ontologenius#HasInRightHandEffect"/>
         <rdfs:label xml:lang="en">{Agent} @Pick {Pickable}</rdfs:label>
    </owl:Class>

    <!-- urn:absolute:ontologenius#PlaceAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#PlaceAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PhysicalAction"/>
        <rdfs:hasEffect rdf:resource="urn:absolute:ontologenius#IsOnTopOfEffect"/>
         <rdfs:label xml:lang="en">{Agent} @Place {Pickable} on {Support}</rdfs:label>
    </owl:Class>
    
     <!-- urn:absolute:ontologenius#ScanAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#ScanAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PhysicalAction"/>
        <rdfs:hasEffect rdf:resource="urn:absolute:ontologenius#IsScannedEffect"/>
         <rdfs:label xml:lang="en">{Agent} @Scan {Pickable}</rdfs:label>
    </owl:Class>

     <!-- urn:absolute:ontologenius#HeatWaterAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#HeatWaterAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PhysicalAction"/>
    </owl:Class>
    
     <!-- urn:absolute:ontologenius#HeatWaterInKettle -->

    <owl:Class rdf:about="urn:absolute:ontologenius#HeatWaterInKettle">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#HeatWaterAction"/>
        <rdfs:hasEffect rdf:resource="urn:absolute:ontologenius#isHotInKettle"/>
    </owl:Class>    

    <!-- urn:absolute:ontologenius#human_place_cube -->

    <owl:Class rdf:about="urn:absolute:ontologenius#human_place_cube">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PlaceAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#PickAndPlaceAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#PickAndPlaceAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PhysicalAction"/>
        <rdfs:hasEffect rdf:resource="urn:absolute:ontologenius#IsOnTopOfEffect"/>
         <rdfs:label xml:lang="en">{Agent} @Pick {Pickable} and @Place it on {Support}</rdfs:label>
    </owl:Class>

    <!-- urn:absolute:ontologenius#PickAndPlace -->

    <owl:Class rdf:about="urn:absolute:ontologenius#PickAndPlace">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PickAndPlaceAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#PickAndPlaceStickAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#PickAndPlaceStickAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PickAndPlaceAction"/>
        <rdfs:label xml:lang="en">{Agent} @Pick {Pickable} and @Place it on {Support} and {Support}</rdfs:label>
    </owl:Class>


        <!-- urn:absolute:ontologenius#PickAndPlaceStick -->

    <owl:Class rdf:about="urn:absolute:ontologenius#PickAndPlaceStick">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PickAndPlaceStickAction"/>
        <rdfs:label xml:lang="en">{Agent} @Pick {Pickable} and @Place it on {Support} and {Support}</rdfs:label>
    </owl:Class>

    <!-- urn:absolute:ontologenius#human_pick_cube -->

    <owl:Class rdf:about="urn:absolute:ontologenius#human_pick_cube">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PickAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#robot_pick_cube -->

    <owl:Class rdf:about="urn:absolute:ontologenius#robot_pick_cube">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PickAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#take -->

    <owl:Class rdf:about="urn:absolute:ontologenius#take">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PickAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#remove -->

    <owl:Class rdf:about="urn:absolute:ontologenius#remove">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PickAndPlaceAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#DropAction -->

    <owl:Class rdf:about="urn:absolute:ontologenius#DropAction">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#PhysicalAction"/>
        <rdfs:label xml:lang="en">{Agent} @Drop {Cube}</rdfs:label>
        <rdfs:label xml:lang="en">{Agent} @Drop in {Container}</rdfs:label>
        <rdfs:label xml:lang="en">{Agent} @Drop {Cube} in {Container}</rdfs:label>
    </owl:Class>

    <!-- urn:absolute:ontologenius#human_drop_cube -->

    <owl:Class rdf:about="urn:absolute:ontologenius#human_drop_cube">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#DropAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#robot_drop_cube -->

    <owl:Class rdf:about="urn:absolute:ontologenius#robot_drop_cube">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#DropAction"/>
    </owl:Class>

   <!-- urn:absolute:ontologenius#drop -->

    <owl:Class rdf:about="urn:absolute:ontologenius#drop">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#DropAction"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#robot_tell_human_to_tidy -->

    <owl:Class rdf:about="urn:absolute:ontologenius#robot_tell_human_to_tidy">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#VerbalCommunication"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#robot_congratulate -->

    <owl:Class rdf:about="urn:absolute:ontologenius#robot_congratulate">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#VerbalCommunication"/>
    </owl:Class>

    <!-- urn:absolute:ontologenius#robot_wait_for_human_to_tidy -->

    <owl:Class rdf:about="urn:absolute:ontologenius#robot_wait_for_human_to_tidy">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#Wait"/>
    </owl:Class>

 <!--
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Effect Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
  	<owl:Class rdf:about="urn:absolute:ontologenius#Effect">
    </owl:Class>  
    
    <owl:Class rdf:about="urn:absolute:ontologenius#NonObservableEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#Effect"/>
    </owl:Class>    
        
 	<owl:Class rdf:about="urn:absolute:ontologenius#InstantaneousEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#ObservableEffect"/>
    </owl:Class>    
    
 	<owl:Class rdf:about="urn:absolute:ontologenius#ObservableEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#ObservableEffect"/>
    </owl:Class>            
    
 	<owl:Class rdf:about="urn:absolute:ontologenius#VisualEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#ObservableEffect"/>
    </owl:Class>     
    
    <owl:Class rdf:about="urn:absolute:ontologenius#AuditoryEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#ObservableEffect"/>
    </owl:Class>    
    
    <owl:Class rdf:about="urn:absolute:ontologenius#ThudEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#AuditoryEffect"/>
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#InstantaneousEffect"/>        
    </owl:Class>  
    
    <owl:Class rdf:about="urn:absolute:ontologenius#TactileEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#ObservableEffect"/>
    </owl:Class>  
     
	<owl:Class rdf:about="urn:absolute:ontologenius#HasInHandEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#VisualEffect"/>
    </owl:Class>

	<owl:Class rdf:about="urn:absolute:ontologenius#HasInRightHandEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#HasInHandEffect"/>
    </owl:Class>
     
	<owl:Class rdf:about="urn:absolute:ontologenius#IsOnTopOfEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#VisualEffect"/>
    </owl:Class>

	<owl:Class rdf:about="urn:absolute:ontologenius#IsScannedEffect">
        <rdfs:subClassOf rdf:resource="urn:absolute:ontologenius#NonObservableEffect"/>
    </owl:Class>
     
